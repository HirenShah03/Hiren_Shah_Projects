<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<!-- Root Context: defines shared resources visible to all other web components -->
		<bean id="userDao" class="com.me.finalproj.dao.UserDao" ></bean>
		<bean id="userValidator" class="com.me.finalproj.validator.UserValidator"></bean>

		<bean id="housingDao" class="com.me.finalproj.dao.HousingDao" ></bean>
		<bean id="housing" class="com.me.finalproj.pojo.Housing" ></bean>

		<bean id="productDao" class="com.me.finalproj.dao.ProductDao" ></bean>
		<bean id="product" class="com.me.finalproj.pojo.Product" ></bean>

		<bean id="categoryDao" class="com.me.finalproj.dao.CategoryDao" ></bean>
		<bean id="categoryValidator" class="com.me.finalproj.validator.CategoryValidator" ></bean>
		
</beans>
